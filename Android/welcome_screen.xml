<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/paws0"
    tools:context="com.example.tictacpaw.MainActivity"
    tools:ignore="MergeRootFrame" >

    <RelativeLayout
        android:layout_width="595dp"
        android:layout_height="895dp" >

        <TextView
            android:id="@+id/textView0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="23dp"
            android:text="Welcome to Tic Tac Paw!"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <Button
            android:id="@+id/button15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView0"
            android:layout_below="@+id/textView0"
            android:layout_marginLeft="80dp"
            android:text="Start" />

        <Button
            android:id="@+id/button16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/button15"
            android:layout_below="@+id/button15"
            android:text="Quickest Paws" />

    </RelativeLayout>
        
</FrameLayout>
